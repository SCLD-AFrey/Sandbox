<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:VikingEnterprise.Client.ViewModels.MainApplication"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="VikingEnterprise.Client.Views.MainApplication.UserManagementView">

    <Design.DataContext>
        <!-- ReSharper disable once Xaml.ConstructorWarning -->
        <viewModels:UserManagementViewModel />
    </Design.DataContext>
    
    <Grid>
        <DataGrid Name="MyDataGrid" Items="{Binding Users}" >
        </DataGrid>
    </Grid>
    
    
    
</UserControl>
